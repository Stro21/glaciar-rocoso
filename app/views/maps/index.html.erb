<p id="notice"><%= notice %></p>

<h1>Maps</h1>

<table id="map">
  <thead>
    <tr>
      <th>Name</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th>Url</th>
      <th>Elevation</th>
      <th>Zoom</th>
      <th>Glaciar Rock</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @maps.each do |map| %>
      <tr>
        <td><%= map.name %></td>
        <td><%= map.latitude %></td>
        <td><%= map.longitude %></td>
        <td><%= map.url %></td>
        <td><%= map.elevation %></td>
        <td><%= map.zoom %></td>
        <td><%= map.glaciar_rock %></td>
        <td><%= map.user.username %></td>
        <td><%= link_to 'Show', map %></td>
        <td><%= link_to 'Edit', edit_map_path(map) %></td>
        <td><%= link_to 'Destroy', map, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%#= link_to 'New Map', new_map_path %>

<h2>New post</h2>

<%= render 'form', map: @map %>
<br>
<%= render 'googlemap', map: @map %>
